#include<stdio.h>
char a[1009], b[1009];
int main()
{
	int x, y;
	while (~scanf("%d", &x))
	{
		getchar();
		for (int i = 0; i < x; i++)
		{
			scanf("%c", &a[i]);
			getchar();
		}
		scanf("%d", &y);
		getchar();
		for (int i = 0; i < y; i++)
		{
			scanf("%c", &b[i]);
			getchar();
		}
		printf("%d\n", x * y);
		for (int i = 0; i < x; i++)
		{
			for (int j = 0; j < y; j++)
			{
				printf("<%c,%c>\n", a[i], b[j]);
			}
		}
	}
	return 0;
}
