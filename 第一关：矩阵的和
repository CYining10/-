#include<iostream>
#include<stdlib.h>
int main()
{
	using namespace std;
	int m, n, i = 0, j = 0;
	int a[200][200], b[200][200], sum[200][200];
	while (cin >> m >> n) {
		for (i = 0; i < m; i++) {
			for (j = 0; j < n; j++)
			{
				cin >> a[i][j];
			}
		}
		for (i = 0; i < m; i++) {
			for (j = 0; j < n; j++)
			{
				cin >> b[i][j];
			}
		}
		for (i = 0; i < m; i++) {
			for (j = 0; j < n; j++)
			{
				sum[i][j] = a[i][j] + b[i][j];
			}
		}
		for (i = 0; i < m; i++) {
			for (j = 0; j < n; j++) {
				cout << sum[i][j];
				if (j < n - 1)
					cout << " ";
			}
			cout << endl;
		}
	}
	return 0;
}
