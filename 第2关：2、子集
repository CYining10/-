#include<algorithm>
#include<iostream>
#include<cstdio>
#include<cctype>
#include<set>
using namespace std;
const int maxn = 19;
string s;
int n = 0;
char a[maxn];
int main()
{
	cin >> s;
	for (int i = 0; i < s.length(); i++)
	{
		if (isalpha(s[i]))
		{
			a[n++] = s[i];
		}
	}
	for (int i = 1; i < (1 << n); i++)
	{
		string t = "";
		for (int j = 0; j < n; j++)
		{
			if (i & (1 << j))
			{
				t += a[j];
			}
		}
		cout << "{";
		bool one = 0;
		for (int k = 0; k < t.length(); k++)
		{
			if (!one)one = 1;
			else cout << ",";
			cout << t[k];
		}
		cout << "}" << endl;
	}
	return 0;
}
